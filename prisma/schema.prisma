generator client {
  provider = "prisma-client-js"
}
datasource db {
  provider = "postgresql" // Or your database provider
  url      = env("DATABASE_URL")
}

model User {
  id            Int      @id @default(autoincrement())
  bio           String?
  custody       String   @unique
  displayName   String?
  fid           Int      @unique
  pfpUrl        String?
  username      String   @unique
  verifications String[]
}

